    <script>
        async function addApiKey(){
             let key = {
              OAuthToken: document.getElementById('token_oauth').value,
              Title: document.getElementById('token_title').value,
            };

            let response = await fetch('/Dashboard/CreateYandexDirectAccount', {
                method: 'POST',
                headers: {
                'Content-Type': 'application/json;charset=utf-8'
                },
                body: JSON.stringify(key)
            });

            document.location.reload();
        }

        async function deleteApiKey(id){
             let response = await fetch('/Dashboard/DeleteYandexDirectAccount?accountId='+id, {
                 method: 'DELETE',
             });
             document.location.reload();
        }

           async function setKeySelected(value){
             let response = await fetch('/Dashboard/SelectActiveYaDirectAcc?accountId='+value, {
                 method: 'PUT',
             });
             document.location.reload();
        }

    </script>
